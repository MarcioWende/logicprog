# Definindo duas variáveis para armazenar números
numero1 = 10
numero2 = 5

# Utilizando operadores de comparação para comparar os valores
igual = numero1 == numero2
diferente = numero1 != numero2
maior_que = numero1 > numero2
menor_que = numero1 < numero2
maior_ou_igual = numero1 >= numero2
menor_ou_igual = numero1 <= numero2

# Imprimindo os resultados
print("Número1 é igual a Número2?", igual)
print("Número1 é diferente de Número2?", diferente)
print("Número1 é maior que Número2?", maior_que)
print("Número1 é menor que Número2?", menor_que)
print("Número1 é maior ou igual a Número2?", maior_ou_igual)
print("Número1 é menor ou igual a Número2?", menor_ou_igual)
